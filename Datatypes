iris1<-iris 
iris_sample<-sample(2,nrow(iris1),replace=TRUE,prob = c(0.8,0.2)) 
print(iris_sample)

Train_iris<-iris1[iris_sample==1,]
print(Train_iris)
Test_iris<-iris1[iris_sample==2,]
print(Test_iris)

iris_sample<-sample(3,nrow(iris1),replace=TRUE,prob = c(0.7,0.2,.1))
print()
Train_iris<-iris1[iris_sample==1,]
print(Train_iris)
Test_iris<-iris1[iris_sample==2,]
print(Test_iris)
Test_iris<-iris1[iris_sample==3,]
print(Test_iris)
