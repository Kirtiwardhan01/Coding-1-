### a+2b+2c=20;2a+5b+8c=80;6a+7b+9c=190
coeff<-c(1,2,2,2,5,8,6,7,9)
values<-c(20,80,190)
AA<-matrix(coeff,nrow=3,ncol=3,byrow=TRUE)
BB<-matrix(values,nrow=3,ncol=1)
CC<-solve(AA,BB)
print(CC)

