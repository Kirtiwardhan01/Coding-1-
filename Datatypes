View(mtcars)
filetr_exe<-filter(mtcars,disp==160)
print(filetr_exe)
filetr_exe1<-filter(mtcars,((disp==160)&(am==1)))
print(filetr_exe1)

arrange_exe<-arrange(mtcars,disp)
print(arrange_exe)
arrange_exe<-arrange(mtcars,desc(disp))
print(arrange_exe)

select_exe<-select(mtcars,mpg,cyl)
print(select_exe)

